// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  // output   = "./prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Contactus {
  id      String @id @default(cuid())
  name    String
  email   String
  subject String
  message String @default("")
}
